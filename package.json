{
  "main": "dist/index.js",
  "dependencies": {
    "cache-manager": "^5.2.3",
    "discord.js": "^14.11.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "moment": "^2.29.4"
  },
  "scripts": {
    "build": "rimraf dist && npx tsc && esbuild dist/index.js --bundle --platform=node --outdir=./dist --allow-overwrite --minify",
    "prestart": "npm run build",
    "start": "node dist/index.js prod",
    "preserve": "npm run build",
    "serve": "concurrently \"npx tsc -w\" \"nodemon dist/index.js dev\""
  },
  "devDependencies": {
    "@types/cache-manager": "^4.0.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.4.2",
    "concurrently": "^8.2.0",
    "esbuild": "^0.18.13",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.6"
  }
}
